{
    "startPage":"ListCases",
    "contents":{
        "widget":"GWT.HorizontalPanel",
        "width":"100%",
        "height":"100%",
        "contents":[
            {
                "widget":"ItemLens.PagePanel",
                "cellHorizontalAlignment":"left",
                "cellWidth":"100%",
                "width":"94%",
                "height":"90%"
            }
        ]
    },
    "pages":{
        "ListCases":{
            "contents":{
                "widget":"GWT.VerticalPanel",
                "cellSpacing":10,
                "cellPadding":10,
                "contents":[
                    "#sections.ListCases-welcome",
                    "#sections.ListCases-table"
                ]
            }
        },
        "ViewCase":{
            "contents":{
                "widget":"GWT.VerticalPanel",
                "cellSpacing":10,
                "cellPadding":10,
                "contents":[
                    "#sections.ViewCase-top",
                    "#sections.ViewCase-form"
                ]
            }
        },
        "sections":{
            "ListCases-welcome":{
                "widget":"GWT.DecoratedPopupPanel",
                "contents":{
                    "html":"<h3>Item Hash</h3><p>Welcome to the ItemHash Bugtracker Demo. Below is a list of sample cases.</p>",
                    "width":"630px",
                    "height":"80px"
                }
            },
            "ListCases-table":{
                "widget":"GWT.DecoratedPopupPanel",
                "cellHeight":"100%",
                "contents":{
                    "widget":"ItemLens.PagingScrollTable",
                    "dataPath":"mem:/ItemHash/tickets",
                    "resizePolicy":"fillWidth",
                    "scrollPolicy":"disabled",
                    "pageSize":5,
                    "width":"630px",
                    "height":"250px",
                    "cellPadding":1,
                    "selectionPolicy":"checkbox",
                    "headerBorderWidth":0,
                    "borderWidth":0,
                    "rowCount":true,
                    "postCachedRowCount":0,
                    "preCachedRowCount":0,
                    "columns":[
                        {
                            "key":"id",
                            "header":"Id",
                            "minColumnWidth":40,
                            "maxColumnWidth":50,
                            "preferredColumnWidth":45,
                            "sortable":false,
                            "type":"integer",
                            "horizontalAlignment":"center",
                            "headerHorizontalAlignment":"center"
                        },
                        {
                            "key":"status",
                            "header":"Status",
                            "preferredColumnWidth":70,
                            "minColumnWidth":60,
                            "maxColumnWidth":80,
                            "sortable":true,
                            "type":"string",
                            "horizontalAlignment":"center",
                            "headerHorizontalAlignment":"center"
                        },
                        {
                            "key":"title",
                            "header":"Title",
                            "preferredColumnWidth":200,
                            "minColumnWidth":50,
                            "maxColumnWidth":290,
                            "sortable":true,
                            "type":"itemLink",
                            "page":"ViewCase",
                            "form":"ViewCaseForm",
                            "itemKey":"id",
                            "storePath":"mem:/ItemHash/tickets/",
                            "headerHorizontalAlignment":"center"
                        },
                        {
                            "key":"category",
                            "header":"Category",
                            "minColumnWidth":20,
                            "maxColumnWidth":100,
                            "preferredColumnWidth":50,
                            "sortable":true,
                            "horizontalAlignment":"center",
                            "headerHorizontalAlignment":"center"
                        },
                        {
                            "key":"project",
                            "header":"Project",
                            "preferredColumnWidth":75,
                            "minColumnWidth":68,
                            "maxColumnWidth":90,
                            "sortable":true,
                            "horizontalAlignment":"center",
                            "headerHorizontalAlignment":"center"
                        },
                        {
                            "key":"priority",
                            "header":"Priority",
                            "preferredColumnWidth":48,
                            "minColumnWidth":45,
                            "maxColumnWidth":50,
                            "sortable":true,
                            "type":"integer",
                            "headerHorizontalAlignment":"center",
                            "horizontalAlignment":"center"
                        },
                        {
                            "key":"owner",
                            "header":"Owner",
                            "preferredColumnWidth":50,
                            "minColumnWidth":48,
                            "maxColumnWidth":55,
                            "sortable":true,
                            "horizontalAlignment":"center",
                            "headerHorizontalAlignment":"center"
                        }
                    ],
                    "rowColors":[
                        "#FFFFFF",
                        "#DDDDDD"
                    ]
                }
            },
            "ViewCase-top":{
                "widget":"GWT.DecoratedPopupPanel",
                "contents":{
                    "widget":"GWT.VerticalPanel",
                    "contents":[
                        {
                            "widget":"GWT.Html",
                            "width":"630px",
                            "html":"<h3>ViewCase</h3><p>This page shows the current case selected.</p>"
                        }
                    ]
                }
            },
            "ViewCase-form":{
                
            }
        },
        "items":{
            "mem:/ItemHash/tickets/0":{
